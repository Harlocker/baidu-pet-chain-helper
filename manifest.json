{
    "manifest_version": 2,
    "name": "莱茨狗定制版",
    "description": "莱茨狗抢购插件定制版",
    "version": "1.20",
    "background": {
        "page": "background.html"
    },
    "browser_action": {
        "default_icon": "icon.png",
        "default_title": "莱茨狗抢购插件定制版",
        "default_popup": "popup.html"
    },
    "permissions": [
        "tabs",
        "*://*/*",
        "webRequestBlocking",
        "webRequest",
        "browsingData",
        "storage",
        "unlimitedStorage"
    ],
    "icons": {
        "128": "icon.png"
    }
}